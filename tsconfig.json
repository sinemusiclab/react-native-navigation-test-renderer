{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "jsx": "react-native",
    "declaration": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "outDir": "./dist/",
    "sourceMap": true,
    "strict": true,
    "importHelpers": true,
    "types": [
      "jest",
      "react",
      "react-native"
    ]
  },
  "include": ["src"]
}
